/* pages/Login/index.wxss */
page,.Login{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.logo{
  width: 180rpx;
  height: 180rpx;
  margin-top: 450rpx;
  border-radius: 50%;
}
.infoInput{
  width: 600rpx;
  height: 300rpx;
  /* background: rgba(0, 0, 0, 0.3); */
  border-radius: 10rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.btnLogin{
  width: 380rpx;
  height: 85rpx;
  border-radius: 10rpx;
  opacity: 0.6;
  margin-top: 10rpx;
  background: #fff;
  font-size: 35rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 700;
}
.btnLogin text:nth-child(1){
  margin-right: 30rpx;
}
.noPossword{
  margin-top: 30rpx;
  font-size: 27rpx;
  color: #fff;
}
.van-cell{
  background-color: rgba(0, 0, 0, 0.3) !important;
  margin-bottom: 20rpx;
  color: #fff !important;
  border-radius: 10rpx;
}
.van-cell__value {
  color: #fff !important;
}
.van-cell:after{
  border:none !important;
}
.van-field__input{
  color: #fff !important;
}
