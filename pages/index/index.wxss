/**index.wxss**/
.container,page{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background: #f5f5f5;
}
.container navigator{
  display:flex;
  width: 90%;
  height: 80rpx;
  margin-left: 5%;
  border-radius: 10rpx;
  background: #fff;
  margin-bottom: 20rpx;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
  font-size: 30rpx;
  box-sizing: border-box;
}
.container>view image{
  width: 30rpx;
  height: 30rpx;
}